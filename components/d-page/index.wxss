.d-page {
  box-sizing: border-box;
  /* min-height: 100%; */
  background-color: #f2f2f2;
  font-size: calc(1%);
  width: 750rpx;
  height: 100%;
  overflow-x: hidden;
  overflow-y: scroll;
}
.flex {
  width: 750rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.safe-area {
  width: 100%;
  height: 1206rpx;
  position: relative;
}
.no-safe-area {
  width: 100%;
  height: 100%;
  position: relative;
}

page {
  height: 100%;
}
.full {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
