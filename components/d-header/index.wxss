.d-header {
  background-color: #ffffff;
  color: black;
  display: flex;
  flex-direction: column;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
}

.d-header-main {
  flex-grow: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 37rpx;
  position: relative;
}
.btn-back {
  width: 150rpx;
  height: 36rpx;
  position: absolute;
  bottom: 25rpx;
  left: 21rpx;
  padding-left: 36rpx;
  z-index: 101;
}
.back-text {
  bottom: 0;
  font-size: 32rpx;
  color: black;
  font-weight: 500;
  line-height: 36rpx;
}
.back-img {
  position: absolute;
  width: 36rpx;
  height: 36rpx;
  left: 0;
  top: 0;
}
